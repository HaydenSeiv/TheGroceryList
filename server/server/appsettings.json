{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "MongoDB": "mongodb://localhost:27017"
  },
  "MongoDbSettings": {
    "DatabaseName": "golang_db",
    "ItemsCollectionName": "items",
    "UsersCollectionName": "users", 
    "ListsCollectionName": "lists"
  },
  "JwtSettings": {
    "SecretKey": "your-super-secret-jwt-key-here-should-be-at-least-256-bits-long-for-security",
    "Issuer": "TheGroceryListApi",
    "Audience": "TheGroceryListClient",
    "ExpiryHours": 24
  },
  "Environment": "Development",
  "AllowedOrigins": [
    "http://localhost:5173",
    "http://localhost:5174"
  ]
}
