{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MongoDbSettings": {
    "DatabaseName": "golang_db",
    "ItemsCollectionName": "items",
    "UsersCollectionName": "users", 
    "ListsCollectionName": "lists",
    "LayoutsCollectionName": "layouts",
    "AislesCollectionName":  "aisles"
  },
  "JwtSettings": {    
    "Issuer": "TheGroceryListApi",
    "Audience": "TheGroceryListClient",
    "ExpiryHours": 24
  },
  "Environment": "Development",
  "AllowedOrigins": [
    "http://localhost:5173",
    "http://localhost:5174", 
    "https://the-grocery-list.vercel.app"
  ]
}
