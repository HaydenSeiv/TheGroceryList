{
  "dependencies": {
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^7.6.0"
  },

  "scripts": {
    "install:all": "cd client && npm install && cd ../server/server && dotnet restore",
    "dev:client": "cd client && npm run dev",
    "dev:server": "cd server/server && dotnet run",
    "test:client": "cd client && npm run test:run",
    "test:server": "cd server && dotnet test",
    "test:all": "npm run test:client && npm run test:server",
    "build:client": "cd client && npm run build",
    "build:server": "cd server/server && dotnet build"
  }
}

